<%= simple_form_for @rule, wrapper: :default do |f| %>
    <%= f.input :name, default%>
    <%= f.input :klass_name %>
    <%= f.input :klass_finder_method %>
    <%= f.input :template_id, collection: Rooler::Template.all %>
    <%= f.input :check_frequency %>
    <%= f.input :method_params %>
    <%= f.button :submit %>
<% end %>
